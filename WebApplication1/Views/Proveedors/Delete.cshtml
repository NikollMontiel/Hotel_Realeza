@model WebApplication1.Models.Proveedor

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar</h2>

<h3>Está seguro que desea eliminar este registro?</h3>
<div>
    <h4>Proveedor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Primer nombre proveedor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PNP)
        </dd>

        <dt>
            Segundo nombre proveedor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SNP)
        </dd>

        <dt>
            Primer apellido proveedor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PAP)
        </dd>

        <dt>
            Segundo apellido proveedor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SAP)
        </dd>

        <dt>
            Teléfono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TelP)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Volver a la lista", "Index")
        </div>
    }
</div>
